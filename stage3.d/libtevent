
#requires libtalloc

rpmi libtevent

grep -v 'doxygen' < $HOME/rpmbuild/SPECS/libtevent.spec > $HOME/rpmbuild/SPECS/libtevent.spec2
grep -v 'doc/man' < $HOME/rpmbuild/SPECS/libtevent.spec2 > $HOME/rpmbuild/SPECS/libtevent.spec
grep -v 'man3' < $HOME/rpmbuild/SPECS/libtevent.spec > $HOME/rpmbuild/SPECS/libtevent.spec2
mv $HOME/rpmbuild/SPECS/libtevent.spec2 $HOME/rpmbuild/SPECS/libtevent.spec

rpmb -bb libtevent

rpminst libtevent libtevent-devel
